{
    "parser": "babel-eslint",
    "plugins": [],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {}
    },
    "env": {
        "amd": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "comma-dangle": 0,
        "quotes": [2, "double"],
        "no-undef": 1,
        "global-strict": 0,
        "no-extra-semi": 1,
        "no-underscore-dangle": 0,
        "no-console": 1,
        "no-unused-vars": 1,
        "no-trailing-spaces": [1, { "skipBlankLines": true }],
        "no-unreachable": 1,
        "no-alert": 0
    }
}